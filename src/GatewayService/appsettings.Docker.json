{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Error",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "identity": {
        "Destinations": {
          "userApi": {
            "Address": "http://identity-service"
          }
        }
      },
      "profile": {
        "Destinations": {
          "profileApi": {
            "Address": "http://profile-service"
          }
        }
      },
      "stories": {
        "Destinations": {
          "profileApi": {
            "Address": "http://stories-service"
          }
        }
      }
    }
  }
}

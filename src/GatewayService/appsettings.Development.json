{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "userRegister": {
        "ClusterId": "usersCluster",
        "Match": {
          "Path": "/user/{**catch-all}",
          "Methods": ["POST"]
        }
      }
    },

    "Clusters": {
      "usersCluster": {
        "Destinations": {
          "usersCluster/destination": {
            "Address": "http://localhost:5171"
          }
        }
      }
    }
  }
}

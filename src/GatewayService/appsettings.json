{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Error",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ClientApp": "http://localhost:3000",
  "ReverseProxy": {
    "Routes": {
      "authRead": {
        "ClusterId": "auth",
        "Match": {
          "Path": "/api/v1/auth/{**catch-all}",
          "Methods": [
            "GET"
          ]
        }
      },
      "authWrite": {
        "ClusterId": "auth",
        "Match": {
          "Path": "/api/v1/auth/{**catch-all}",
          "Methods": [
            "POST",
            "PUT",
            "DELETE"
          ]
        }
      },
      "profileRead": {
        "ClusterId": "profilesApi",
        "Match": {
          "Path": "/api/v1/profiles/{userId:guid}/{**catch-all}",
          "Methods": [
            "GET"
          ]
        }
      },
      "profileCountriesRead": {
        "ClusterId": "profilesApi",
        "Match": {
          "Path": "/api/v1/profiles/countries",
          "Methods": [
            "GET"
          ]
        }
      },
      "profileWrite": {
        "ClusterId": "profilesApi",
        "Match": {
          "Path": "/api/v1/profiles/{userId:guid}/{**catch-all}",
          "Methods": [
            "POST",
            "PUT"
          ]
        }
      },
      "profileImage": {
        "ClusterId": "profilesApi",
        "Match": {
          "Path": "/api/v1/profiles/{userId:guid}/image/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
          ]
        }
      },
      "storiesRead": {
        "ClusterId": "storiesApi",
        "Match": {
          "Path": "/api/v1/stories/{**catch-all}",
          "Methods": [
            "GET"
          ]
        }
      },
      "storiesWrite": {
        "ClusterId": "storiesApi",
        "Match": {
          "Path": "/api/v1/stories/{**catch-all}",
          "Methods": [
            "POST",
            "PUT",
            "DELETE"
          ]
        }
      }
    }
  }
}
